# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Authentication Credentials (Change these!)
NEXT_PUBLIC_LOGIN_USERNAME=your_username
NEXT_PUBLIC_LOGIN_PASSWORD=your_password
NEXT_PUBLIC_LOGIN_OTP=your_otp

# Edit Source Profiles Password (Change this!)
NEXT_PUBLIC_DELETE_PASSWORD=your_delete_password


# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5001

# Campaign Configuration
# Daily target is calculated as: NUM_VA_TABLES * PROFILES_PER_TABLE (e.g., 80 * 180 = 14,400)
NEXT_PUBLIC_NUM_VA_TABLES=80
NEXT_PUBLIC_PROFILES_PER_TABLE=180
